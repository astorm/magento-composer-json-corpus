{
    "name":        "smaex/customer-group-payments",
    "description": "Restricts payment methods to specific customer groups in Magento 2.",
    "type":        "magento2-module",
    "version":     "2.0.0",
    "license":     "MIT",
    "keywords":
    [
        "magento", "payment", "customer group"
    ],
    "require":
    {
        "php":                             "^7.1",
        "magento/framework":               "^102.0",
        "magento/module-customer":         "^102.0",
        "magento/module-payment":          "^100.2",
        "magento/module-quote":            "^101.0",
        "smaex/additional-payment-checks": "^1.0"
    },
    "require-dev":
    {
        "phpunit/phpunit": "^7.0"
    },
    "autoload":
    {
        "files":
        [
            "registration.php"
        ],
        "psr-4":
        {
            "Smaex\\CustomerGroupPayments\\": ""
        }
    }
}
