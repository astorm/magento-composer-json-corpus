{
  "name": "cristiano-pacheco/module-base",
  "description": "Cristiano Pacheco Magento 2 Base Module",
  "version": "1.0.0",
  "type": "magento2-module",
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "Cristiano Pacheco",
      "email": "contato@cristianopacheco.com.br"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "magento/framework": "100.1.*|101.0.*|102.0.*",
    "magento/module-store": "100.1.*|100.2.*|101.0.*",
    "magento/module-config": "100.1.*|101.0.*|101.1.*",
    "magento/module-backend": "100.1.*|100.2.*|101.0.*"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "CristianoPacheco\\ModuleBase\\": ""
    }
  },
  "keywords" : [
		"magento2",
		"magento 2 module"
	]
}
