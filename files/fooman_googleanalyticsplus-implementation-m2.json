{
    "name": "fooman/googleanalyticsplus-implementation-m2",
    "license": "OSL-3.0",
    "description": "Additional functionality for the default Magento Google Analytics",
    "type": "magento2-module",
    "version": "105.0.0",
    "autoload": {
        "psr-4": {
            "Fooman\\GoogleAnalyticsPlus\\": "src/"
        },
        "files": [
            "src/registration.php"
        ]
    },
    "require": {
        "php": "~5.6.0|7.0.2|7.0.4|~7.0.6|~7.1.0|~7.2.0|~7.3.0",
        "magento/module-google-analytics": "^100.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1 | ^6.2",
        "fooman/magento2-phpunit-bridge": "^0.7.0",
        "captainhook/captainhook": "^4.2",
        "magento/magento-coding-standard": "*",
        "fooman/phpstan-magento2-magic-methods": "^0.6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo-magento-mirror.fooman.co.nz/"
        }
