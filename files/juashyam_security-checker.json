    "require": {
        "php": "~5.5.0|~5.6.0|~7.0.0|~7.1.0|~7.2.0|~7.3.0",
        "magento/framework": "^100.1.0|101.0.*|^102.0.0",
        "sensiolabs/security-checker": "^5.0.3"
    },
    "type": "magento2-module",
    "version": "1.0.0",
    "license": [
        "MIT"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Juashyam\\SecurityChecker\\": ""
        }
    }
}
