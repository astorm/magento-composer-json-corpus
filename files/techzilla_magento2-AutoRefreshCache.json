{
    "name": "cyberpunkspike/module-autorefreshcache",
    "description": "Magento2 module to refresh invalid caches via cron",
    "keywords": ["magento2","cache"],
    "type": "magento2-module",
    "license": "MIT",
    "authors": [
        {
            "email": "cyberpunkspike@gmail.com",
            "name": "J.M. Becker"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "~7.1.3||~7.2.0",
        "magento/framework": "102.0.*"
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "autoload": {
        "psr-4": {
            "Cyberpunkspike\\AutoRefreshCache\\": ""
        },
        "files": [
            "registration.php"
        ]
    }
}