{
    "name": "marellocommerce/magento2-marello-bridge",
    "description": "Marello Bridge Extension for Magento 2",
    "homepage": "https://github.com/marellocommerce/magento2-marello-bridge.git",
    "authors": [
        {
            "name": "Marello",
            "homepage": "http://www.marello.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "magento/framework": "100.1.*",
        "marellocommerce/marello-bridge-api": "1.1.*"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Marello\\Bridge\\": ""
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
