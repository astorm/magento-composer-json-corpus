  "version": "1.1.4",
  "minimum-stability": "dev",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Julien Loizelet",
      "email": "julienloizelet@okaeli.com",
      "homepage": "http://okaeli.com",
      "role": "lead"
    }
