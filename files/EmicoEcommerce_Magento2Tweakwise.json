{
    "name": "emico/tweakwise",
    "license": "OSL-3.0",
    "description": "Magento 2 module for Tweakwise integration",
    "require": {
        "php": "~7.0|~7.1",
        "emico/tweakwise-export": "~1"
    },
    "require-dev": {
        "malukenho/docheader": "~0.1",
        "phpro/grumphp": "^0.11.1"
    },
    "type": "magento2-module",
    "scripts": {
        "docheader": "docheader check ./src"
    },
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Emico\\Tweakwise\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ]
}
