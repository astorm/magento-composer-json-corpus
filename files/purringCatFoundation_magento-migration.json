{
  "name": "pcf/magento-migration",
  "description": "magento phinx migration module",
  "type": "magento2-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Bernard",
      "email": "b.vanderesch@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.0",
    "magento/framework": "100.*",
    "robmorgan/phinx": "^0.8.1",
    "mikehaertl/php-shellcommand": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.0",
    "squizlabs/php_codesniffer": "^2.8",
    "phpunit/phpunit-mock-objects": "2.1.*"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "PCF\\MagentoMigration\\": ""
    }
  }
}
