{
    "name": "ampersand/category-code",
    "description": "Add a unique identifier to categories so that they may be identified by external systems",
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        "ampersand-m2": {
            "type": "composer",
            "url": "https://composer.ampersandweb.co.uk/m2"
        }
    },
    "require": {
        "magento/framework": "^100.1.0|^101|^102",
        "magento/module-catalog": "^100|^101|^102|^103",
        "snowio/magento2-lock": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "type": "magento2-module",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Ampersand\\CategoryCode\\": ""
        }
    }
}
