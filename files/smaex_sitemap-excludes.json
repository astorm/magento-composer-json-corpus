{
    "name":        "smaex/sitemap-excludes",
    "description": "Excludes selected products from XML sitemaps in Magento 2.",
    "type":        "magento2-module",
    "version":     "1.0.1",
    "license":     "MIT",
    "keywords":
    [
        "magento", "seo", "sitemap"
    ],
    "require":
    {
        "php":                    "^7.2",
        "magento/framework":      "^102.0",
        "magento/module-catalog": "^103.0",
        "magento/module-eav":     "^102.0",
        "magento/module-sitemap": "^100.3",
        "magento/module-store":   "^101.0"
    },
    "require-dev":
    {
        "phpunit/phpunit": "^6.5"
    },
    "autoload":
    {
        "files":
        [
            "registration.php"
        ],
        "psr-4":
        {
            "Smaex\\SitemapExcludes\\": ""
        }
    }
}
