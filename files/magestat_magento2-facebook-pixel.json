{
    "name": "magestat/module-facebook-pixel",
    "description": "This extension allow you integrate the Facebook pixel to track your website visitors' actions.",
    "keywords": [
        "magento 2",
        "magento extension",
        "facebook pixel",
        "magento facebook",
        "pixel track",
        "fb pixel"
    ],
    "type": "magento2-module",
    "license": [
        "OSL-3.0"
    ],
    "version": "1.0.2",
    "support": {
        "issues": "https://github.com/magestat/magento2-facebook-pixel/issues/"
    },
    "require": {
        "php": "7.0.2|7.0.2|7.0.4|~7.0.6|~7.1.0|~7.2.0",
        "magento/module-backend": "100.0.*|100.1.*|100.2.*|100.3.*|101.0.*|101.0.*",
        "magento/framework": "100.0.*|100.1.*|100.2.*|100.3.*|101.0.*|102.0.*"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable",
        "phpspec/phpspec": "^2.5",
        "squizlabs/php_codesniffer": "3.0.1"
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Magestat\\FacebookPixel\\": ""
        }
    }
}
