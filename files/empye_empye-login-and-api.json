{
  "name": "empye/empye-login-and-api",
  "description": "Official Magento2 Plugin login using social network, Qr Code and additional extended and simplified api features",
  "type": "magento2-module",
  "version": "1.0.0",
  "license": [
    "Apache-2.0"
  ],
  "require": {
    "php": "~5.5.0|~5.6.0|~7.0.0",
    "magento/framework": "*",
    "magento/module-customer": "*",
    "magento/module-store": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "2.*",
    "guzzlehttp/guzzle": "^6.0",
    "phpunit/phpunit": "4.1.0",
    "behat/behat": "^3.1.0",
    "behat/mink": "^1.7.1",
    "behat/mink-extension": "^2.2",
    "behat/mink-goutte-driver": "^1.2.1",
    "behat/mink-selenium2-driver": "^1.3.1",
    "sensiolabs/behat-page-object-extension": "^2.0.0",
    "bex/behat-magento2-init": "dev-master",
    "ciaranmcnulty/behat-stepthroughextension": "dev-master"
  },
  "authors": [
    {
      "name": "Chandirasekaran",
      "email": "bootstrapid@gmail.com",
      "homepage": "https://www.empye.com/",
      "role": "Developer"
    }
  ],
  "support": {
      "email": "chandirasekaranid@gmail.com",
      "forum": "https://www.empye.com/forum",
      "issues": "https://www.github.com/empye/magento/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    },
    {
      "type": "git",
      "url": "https://github.com/jamescowie/behat-magento2-init.git"
    }
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "files": [
      "src/Login/registration.php",
      "src/Api/registration.php"
    ],
    "psr-4": {
      "Empye\\Login\\": "src/Login",
      "Empye\\Api\\": "src/Api"
    }
  }
}
