{
    "name": "tkotosz/catalog-router-magento2",
    "description": "Tkotosz - Catalog Router",
    "type": "magento2-module",
    "version": "1.0.2",
    "repositories": {
        "magento-repo": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tibor Kotosz",
            "email": "kotosy@gmail.com",
            "homepage": "https://github.com/tkotosz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "magento/framework": "^100.1|^101.0",
        "magento/module-catalog": "^101.0|^102.0",
        "magento/module-catalog-url-rewrite": "^100.1|^100.2"
    },
    "require-dev": {
        "behat/behat": "^3.3"
    },
    "config": {
        "bin-dir": "bin",
        "use-include-path": true
    },
    "autoload": {
        "files": [
            "src/Tkotosz/CatalogRouter/registration.php"
        ],
        "psr-0": {
            "Tkotosz\\CatalogRouter\\": "src"
        }
    }
}
