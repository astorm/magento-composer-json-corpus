{
    "name": "kodbruket/magento2-gotosku",
    "description": "Module for Magento 2.",
    "version": "1.0.0",
    "type": "magento2-module",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "authors": [
        {
            "name": "Andreas Karlsson",
            "email": "andreas@kodbruket.se",
            "homepage": "https://kodbruket.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6.0|~7.0.0",
        "magento/module-catalog": "*",
        "magento/framework": "*",
        "magento/zendframework1": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.5",
        "sebastian/phpcpd": "^2.0"
    },
    "autoload": {
        "files": ["registration.php"],
        "psr-4": {
            "Kodbruket\\GoToSku\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodbruket\\GoToSku\\": ""
        }
    },
    "scripts": {
        "phpunit": "phpunit",
        "phpcs": "phpcs"
    }
}