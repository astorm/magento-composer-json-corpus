{
  "name": "apruve/module-magento2",
  "description": "A Magento 2 module to integrate with the Apruve B2B credit network",
  "type": "magento2-module",
  "version": "1.1.2",
  "license": [
    "MPL-2.0"
  ],
  "require": {
    "php": "~7.1"
  },
  "support": {
    "email": "support@apruve.com",
    "issues": "https://github.com/apruve/apruve-magento2/issues",
    "source": "https://github.com/apruve/apruve-magento2",
    "docs": "https://docs.apruve.com/"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Apruve\\Payment\\": ""
    }
  },
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com"
    }
  }
}
