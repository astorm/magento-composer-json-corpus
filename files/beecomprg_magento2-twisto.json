{
  "name": "beecomprg/module-twisto",
  "description": "Magento 2 Twisto payment gateway",
  "type": "magento2-module",
  "license": "MIT",
  "version": "0.1.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Beecom",
      "email": "lukas@beecom.io"
    }
  ],
  "keywords": [
    "magento",
    "magento2",
    "payment",
    "checkout",
    "fulfillment"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "config": {
    "platform": {
      "php": "7.1"
    }
  },
  "require": {
    "magento/module-checkout": "~100.2|~100.3",
    "beecomprg/magento-2-module-core": ">=1.0.3",
    "giggsey/libphonenumber-for-php": "8.10.15",
    "twisto-payments/twisto": "2.*"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Beecom\\Twisto\\": ""
    }
  }
}
